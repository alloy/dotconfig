[alias]
  ba  = branch -a
  bd  = branch -D
  ch  = checkout
  chb = checkout -b
  chm = checkout master
  st  = status
  ca  = commit --amend
  ci  = commit -v
  cia = commit -a -v
  r   = rebase -i
  wtf = !git-wtf

[core]
  excludesfile = ~/.config/git/ignore

[help]
  autocorrect = 1

[push]
  default = current

